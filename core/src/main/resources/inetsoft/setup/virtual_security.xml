<virtualSecurityProvider>
  <FSUser class="inetsoft.sree.security.FSUser">
    <name><![CDATA[{{adminUsername}}]]></name>
    <active><![CDATA[true]]></active>
    <organization><![CDATA[host-org]]></organization>
    <locale><![CDATA[]]></locale>
    <password algorithm="bcrypt"><![CDATA[{{adminPassword}}]]></password>
    <emails></emails>
    <roles>
      <role><![CDATA[Administrator - __GLOBAL__]]></role>
    </roles>
    <groups></groups>
  </FSUser>
</virtualSecurityProvider>